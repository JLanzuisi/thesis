@article{hamkins_generalizations_2012,
	title = {Generalizations of the Kunen inconsistency},
	volume = {163},
	issn = {0168-0072},
	url = {https://www.sciencedirect.com/science/article/pii/S0168007212000966},
	doi = {10.1016/j.apal.2012.06.001},
	abstract = {We present several generalizations of the well-known Kunen inconsistency that there is no nontrivial elementary embedding from the set-theoretic universe V to itself. For example, there is no elementary embedding from the universe V to a set-forcing extension V[G], or conversely from V[G] to V, or more generally from one set-forcing ground model of the universe to another, or between any two models that are eventually stationary correct, or from V to {HOD}, or conversely from {HOD} to V, or indeed from any definable class to V, among many other possibilities we consider, including generic embeddings, definable embeddings and results not requiring the axiom of choice. We have aimed in this article for a unified presentation that weaves together some previously known unpublished or folklore results, several due to Woodin and others, along with our new contributions.},
	pages = {1872--1890},
	number = {12},
	journaltitle = {Annals of Pure and Applied Logic},
	shortjournal = {Annals of Pure and Applied Logic},
	author = {Hamkins, Joel David and Kirmayer, Greg and Perlmutter, Norman Lewis},
	urldate = {2023-07-07},
	date = {2012-12-01},
	langid = {english},
	keywords = {Definability, Elementary embeddings, Forcing, Kunen inconsistency},
	file = {ScienceDirect Full Text PDF:C\:\\Users\\jlanz\\Zotero\\storage\\EQUAIRVW\\Hamkins et al. - 2012 - Generalizations of the Kunen inconsistency.pdf:application/pdf;ScienceDirect Snapshot:C\:\\Users\\jlanz\\Zotero\\storage\\N6TRXQDF\\S0168007212000966.html:text/html},
