
@article{tarski_problems_1966,
	title = {Some Problems and Results Relevant to the Foundations of Set Theory},
	volume = {44},
	url = {https://www.sciencedirect.com/science/article/pii/S0049237X09705774},
	series = {Logic, Methodology and Philosophy of Science},
	abstract = {This chapter reports on some meta-mathematical results obtained by William P. Hanf (as a solution of a problem suggested by the author) and some mathematical problems involving inaccessible cardinals for which a partial solution are found recently. As a theoretical basis for the discussion, the chapter selects one of the well known axiomatic systems of set theory, the Zermelo-Fraenkel system (with the regularity axiom) or the Bernays system. Numerous problems in set theory and related domains are well known. Each of the problems consists in determining all the (infinite) cardinals that have a given property P. It is known that the smallest infinite cardinal, ω, does not have this property. It has also been known that all the accessible cardinals—for example, all those that are not inaccessible have the property P. It seems that the solution of the problem for inaccessible cardinals may require an introduction of some new set-theoretical axioms that could differ in their character not only from the usual axioms of set theory but also from the generalized continuum hypothesis or various existential hypotheses that assure the existence of inaccessible cardinals. The significance of many problems that are included in the chapter is enhanced by the simplicity of their formulation and the clarity of their mathematical content.},
	pages = {125--135},
	journaltitle = {Studies in Logic and the Foundations of Mathematics},
	author = {Tarski, Alfred},
	editor = {Nagel, Ernest and Suppes, Patrick and Tarski, Alfred},
	urldate = {2023-11-21},
	date = {1966-01-01},
	doi = {10.1016/S0049-237X(09)70577-4},
}

